<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:viewModel="clr-namespace:DiplomaProject.MVVM.ViewModel"
                    xmlns:view="clr-namespace:DiplomaProject.MVVM.View">

    <DataTemplate DataType="{x:Type viewModel:TheoryViewModel}">
        <view:TheoryView />
    </DataTemplate>
    <DataTemplate DataType="{x:Type viewModel:HelloViewModel}">
        <view:HelloView/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type viewModel:ChooseTestViewModel}">
        <view:ChooseTestView/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type viewModel:EcpTestViewModel}">
        <view:EcpTestView/>
    </DataTemplate>

</ResourceDictionary>